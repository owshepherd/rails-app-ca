<h1>Admin Use only</h1>
<h1>Edit Cuisine</h1>

<form action="<%= cuisine_path(params[:id]) %>" method="POST" >
    <input type="hidden" value="<%= form_authenticity_token %>" name="authenticity_token" />

    <input type="hidden" name="_method" value="patch" />

    <label for="cuisine_type">cuisine_type</label>
    <input type="text" name="cuisine_type" id="cuisine_type" value="<%= @cuisine.cuisine_type %>" />

    <input type="submit" value="Create" />
</form>

<p><%= link_to "Cusine List", cuisines_path %></p>