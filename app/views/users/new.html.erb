<h1>Admin Use only</h1>
<h1>Create a New User</h1>

<form action="<%= user_path(params[:id]) %>" method="POST" enctype="multipart/form-data">
    <input type="hidden" value="<%= form_authenticity_token %>" name="authenticity_token" />

    <label for="email">email</label>
    <input type="text" name="email" id="email" value="<%= @user.email %>" />

    <label for="username">username</label>
    <input type="text" name="username" id="username" value="<%= @user.username %>"/>

    <label for="firstname">First name</label>
    <input type="text" name="firstname" id="firstname" value="<%= @user.firstname %>"/>

    <label for="lastname">Last name</label>
    <input type="text" name="lastname" id="lastname" value="<%= @user.lastname %>"/>

    <label for="dob">dob</label>
    <input type="text" name="dob" id="dob" value="<%= @user.dob %>"/>

    <label for="avatar">avatar</label>
    <input type="text" name="uploaded_image" id="uploaded_image" value="<%= @user.uploaded_image %>"/>

    <input type="submit" value="Update" />
</form>

<p><%= link_to "<< Back", :back %></p>
<p><%= link_to "User List", users_path %></p>